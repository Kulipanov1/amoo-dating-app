{
  "expo": {
    "name": "Amoo Dating App",
    "slug": "amoo-dating-app",
    "description": "A modern dating app for meaningful connections - Development Branch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#8A2BE2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amoo.dating"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#8A2BE2"
      },
      "package": "com.amoo.dating"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization",
      "expo-image-picker",
      "expo-camera",
      "expo-location",
      "expo-av"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    }
  }
} 